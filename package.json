{
  "name": "Marionette",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js"
  },
  "browser": {
    "backbone": "./bower_components/backbone/backbone.js",
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "underscore": "./bower_components/underscore/underscore.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:jQuery",
        "underscore:_"
      ]
    },
    "jquery": {
      "exports": "jQuery"
    },
    "underscore": {
      "exports": "_"
    }
  },
  "dependencies": {
    "express": "3.4.0",
    "mongoose": "~4.0.1"
  },
  "devDependencies": {
    "mongodb": "~2.0.27",
    "gulp": "~3.8.10",
    "gulp-jshint": "~1.9.2",
    "gulp-uglify": "~1.1.0",
    "lodash": "~3.1.0",
    "gulp-concat": "~2.4.3",
    "gulp-watch": "~4.1.1",
    "gulp-plumber": "~0.6.6",
    "yargs": "~2.3.0",
    "browserify": "~9.0.8",
    "hbsfy": "~2.2.1",
    "vinyl-source-stream": "~1.1.0",
    "gulp-sourcemaps": "~1.5.2",
    "watchify": "~3.1.1",
    "gulp-util": "~3.0.4",
    "gulp-browserify": "~0.5.1",
    "browserify-shim": "~3.8.5"
  }
}
